/**index.wxss**/
page {
  background-color: #fff;
}
.container{
  width: 750rpx;
  box-sizing: border-box;
  padding-bottom: 120rpx;
}
.banner-img{
  /*width: 695rpx;
  height: 312rpx;*/
  border-radius: 20rpx;
  /*margin: 34rpx auto;*/
  display: block;
}
.notice-wrap{
  width: 700rpx;
  margin: 0 auto;
  margin-top: 20rpx;
  padding: 10rpx 20rpx;
  border: 1rpx solid #ccc;
  border-radius: 5rpx;
  box-shadow: 3rpx 3rpx #eee;
  background-color: white;
}
.notice-swiper{
  width: 100%;
  height: 50rpx;
}
.one-notice{
  font-size: 24rpx;
  display: flex;
  align-items: center;
}
.one-notice .btn{
  display: inline-block;
  border: 1rpx solid #ccc;
  padding: 5rpx;
  margin-right: 10rpx;
}
.one-notice .label{
  display: inline-block;
  max-width: 570rpx;
  white-space: nowrap;
  text-overflow: ellipsis;
  overflow: hidden;
}


.one-card{
  /*border-bottom: 1rpx solid #ccc;*/
  /*box-shadow: 5rpx 5rpx 5rpx #ddd;*/
  width: 750rpx;
  margin: 0rpx auto;
  padding: 30rpx 10rpx 40rpx 10rpx;
  background-color: white;
}
.one-card .top{
  display: flex;
  align-items: top;
  padding: 0 25rpx;
  margin-bottom: 20rpx;
}
.one-card .middle{
  padding: 0 25rpx;
}
.one-card .function{
  padding: 0 25rpx;
}
.one-card .left{
  margin-right: 20rpx;
}

.one-card .images-more{
  display: flex;
  justify-content: flex-start;
  flex-wrap: wrap;
  margin-top: 10rpx;
}
.one-card .images-more image{
  width:210rpx;
  height:210rpx;
  margin-bottom: 10rpx;
  margin-right:20rpx;
}
.one-card .title{
  color: rgb(51,51,51);
  font-size: 28rpx;
  line-height: 40rpx;
  font-weight: 600;
}
.one-card .content{
  color: rgb(51,51,51);
  font-size: 24rpx;
  line-height: 40rpx;
}
.one-card .name{
  font-size: 32rpx;
  line-height: 40rpx;
  color: #121212;
  font-weight: 600;
}
.one-card .time{
  font-size: 20rpx;
  line-height: 38rpx;
  color: rgb(147,147,147);
}
.one-card .utag{
  font-size: 20rpx;
  font-weight: normal;
  /*border: 1rpx solid #ccc;*/
  padding: 0 10rpx;
  margin-left: 20rpx;
  height: 32rpx;
  line-height: 32rpx;
  background: #FFE9E6;
  border-radius: 16rpx;
}
.one-card .header-img{
  display: inline-block;
  width: 90rpx;
  height: 90rpx;
  border-radius: 50rpx;
  margin-right: 5rpx;
}
.function .sharebtn{
  padding: 0;
  display: flex;
  align-items: center;
  width: 130rpx;
  border: none;
  outline: none;
  background-color: transparent;
  font-size: 26rpx;
  line-height: 26rpx;
  color: #595C69;
}

.function{
  padding: 20rpx 30rpx 0rpx 10rpx;
  margin-top: 10rpx;
  /*border-top: 1rpx solid #eee;*/
  display: flex;
  align-items: center;
  justify-content: space-between;
  font-size: 26rpx;
  line-height: 26rpx;
  color: #595C69;
}
.function view{
  display: flex;
  align-items: center;
  line-height: 50rpx;
  height: 50rpx;
}
.function .icon{
  font-size: 26rpx;
  line-height: 26rpx;
  margin-right: 10rpx;
  width: 36rpx;
  height: 36rpx;
  /*display: inline-block;*/
}

.userinfo{
  position: fixed;
  z-index: 100;
  width: 100%;
  height: 100%;
  top: 0;
  left: 0;
}
.userinfo .mask{
  position: fixed;
  z-index: 110;
  width: 100%;
  height: 100%;
  background-color: rgba(0,0,0,0.8);
}
.userinfo .content{
  position: absolute;
  width: 600rpx;
  height: 400rpx;
  padding: 30rpx;
  top: 50%;
  left: 50%;
  margin-top: -150rpx;
  margin-left: -300rpx;
  background-color: white;
  z-index: 200;
  border-radius: 30rpx;
}
.userinfo .title{
  text-align: center;
  font-size: 34rpx;
  line-height: 64rpx;
}
.userinfo .userinfo-btn {
  width: 80%;
  margin-top: 50rpx;
}


.footer{
  position: fixed;
  width: 100%;
  height: 100rpx;
  bottom: 0;
  display: flex;
  align-items: center;
  justify-content: space-between;
  text-align: center;
  /*border-top: 1px solid #ccc;*/
  background-color: white;
  color: #979797;
  font-size: 20rpx;
  line-height: 20rpx;
}
.footer .middle{
/*  border-left: 1px solid #ccc;
  border-right: 1px solid #ccc;*/
}
.footer .navicon{
  width: 40rpx;
  height: 40rpx;
  margin: 0 auto;
}
.footer .navicon-mine{
  width: 40rpx;
  height: 40rpx;
  margin: 0 auto;
}
.footer .nav-add-icon{
  width: 100rpx;
  height: 100rpx;
  margin: 0 auto;
  margin-top: -70rpx;
  border-radius: 80rpx;
  border: 15rpx solid #fff;
}
.footer .onenav{
  width: 33%;
  text-align: center;
  font-size: 32rpx;
  /*line-height: 90rpx;*/
}
.footer .word{
  font-size: 20rpx;
  margin-top: 20rpx;
}
.footer .m-word{
  margin-top: 0rpx;
}


.reginfo{
  position: fixed;
  z-index: 200;
  width: 100%;
  height: 100%;
  top: 0;
  left: 0;
  font-size: 26rpx;
}
.reginfo .p{
  line-height: 32rpx;
  margin-top: 20rpx;
}
.reginfo .oneline{
  display: flex;
  align-items: center;
  min-height: 80rpx;
  margin-top: 20rpx;
}
.reginfo .text{
  width: 200rpx;
}
.reginfo .input{
  width:300rpx;
  border:1px solid #ccc;
  padding:5rpx 15rpx;
  border-radius:5rpx;
  box-sizing: border-box;
}
.reginfo .biginput{
height:70rpx;
line-height:70rpx;
  width: 500rpx;
}
.smalltip{
  font-size: 20rpx;
  color: #ccc;
}
.reginfo .save{
  width: 300rpx;
  border: 1px solid #ccc;
  margin-top: 50rpx;
}
.reginfo .mask{
  position: fixed;
  z-index: 210;
  width: 100%;
  height: 100%;
  background-color: rgba(0,0,0,0.8);
}
.reginfo .content{
  position: absolute;
  width: 600rpx;
  /*height: 400rpx;*/
  padding: 30rpx;
  top: 20%;
  left: 50%;
  margin-top: -150rpx;
  margin-left: -300rpx;
  background-color: white;
  z-index: 220;
  border-radius: 10rpx;
}
